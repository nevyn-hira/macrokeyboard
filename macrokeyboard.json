 {
    "device":"/dev/input/by-id/usb-CHICONY_USB_NetVista_Full_Width_Keyboard-event-kbd",
    "modifier_keys":"KEY_LEFTSHIFT,KEY_LEFTCTRL,KEY_KPENTER",
    "disable_capslock": true,
    "force_numlock": true,
    "launch-notify": true,
    "mimetypelists":{
        "list1":{
            "image/png":"gimp",
            "image/svg+xml":"inkscape",
            "image/jpeg":"gimp",
            "text/plain":"pluma",
            "text/x-python":"pluma",
            "text/css":"pluma",
            "text/csv":"pluma",
            "text/html":"pluma",
            "application/php":"pluma",
            "text/xml":"pluma",
            "image/x-xcf":"gimp"
        }
    },
    "keys":{
        "KEY_SPACE":{
            "action":"holdandrelease",
            "onhold":{
                "action": "XF86Symbol",
                "symbol": "XF86AudioMute"
            },
            "onrelease":{
                "action": "XF86Symbol",
                "symbol": "XF86AudioMute"
            }
        },
        "KEY_A":{
            "action":"sendkeypress",
            "classname":"Navigator.Firefox",
            "keypress":"Key.f11"
        },
        "KEY_ESC":{
            "action":"reload"
        },
        "KEY_ESC+KEY_LEFTSHIFT":{
            "action":"exit"
        },
        "KEY_DELETE":{
            "action":"switchto",
            "classname":"keepass2.KeePass2",
            "executable":"keepass2"
        },
        "KEY_INSERT":{
            "action":"switchto",
            "classname":"code.Code",
            "executable":"code"
        },
        "KEY_HOME":{
            "action":"switchto",
            "classname":"Navigator.Firefox",
            "executable":"firefox"
        },
        "KEY_1":{
            "context_role":{
                "GtkFileChooserDialog":{
                    "action":"gotolocation",
                    "location":"~/Pictures"
                }
            },
            "context_class":{
                "caja.Caja":{
                    "action": "run",
                    "executable":"notify-send",
                    "parameters":"Hello World!"
                }
            },
            "action": "run",
            "executable": "mate-screenshot",
            "parameters": "-ai"
        },
        "KEY_1+KEY_LEFTSHIFT+KEY_LEFTCTRL":{
            "action": "XF86Symbol",
            "symbol": "XF86AudioLowerVolume"
        },
        "KEY_1+KEY_LEFTCTRL":{
            "action": "reload"
        },
        "KEY_2":{
            "action": "keysequence",
            "sequence": "Key.ctrl+l|~/Pictures|Key.enter"
        },
        "KEY_3":{
            "action": "run",
            "executable": "systemctl",
            "parameters": "suspend"
        },
        "KEY_4":{
            "action":"run",
            "executable": "/home/nevyn/macrokeyboard/attachScreenshot"
        },
        "KEY_5":{
            "action": "unicode",
            "code": "b0"
        },
        "KEY_NUMLOCK":{
            "action": "switchto",
            "classname":"vlc.vlc",
            "executable":""
        },
        "KEY_ENTER":{
            "action":"type",
            "content":"Hello, World!"
        }
    }
}